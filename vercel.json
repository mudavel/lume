{
  "routes": [
    {
      "src": "/api/app",
      "dest": "/api/app.js"
    }
  ],
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": ["api/**/*.js"]
      }
    }
  ]
}
